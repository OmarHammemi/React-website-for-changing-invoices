{
  "name": "nestjs-microservice-boilerplate",
  "version": "1.0.0",
  "description": "Digital Rogue Wave",
  "author": "Nizar Chaouachi",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "cache:clear": "ts-node ./node_modules/.bin/typeorm cache:clear"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.5",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "@t00nday/nestjs-pdf": "^2.0.6",
    "@types/pino": "^6.3.6",
    "argon2": "^0.27.1",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "helmet": "^3.23.3",
    "mysql": "^2.18.1",
    "nestjs-pdf": "^1.0.4",
    "nestjs-pino": "^1.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pdfmake": "^0.2.2",
    "pino": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.5",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.17",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.38",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-local": "^1.0.0",
    "husky": "^4.3.6",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
